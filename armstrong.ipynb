   "source": [
    "def is_armstrong(n):\n",
    "    digits = str(n)\n",
    "    length = len(digits)\n",
    "    total = sum(int(digit) ** length for digit in digits)\n",
    "    return total == n\n",
    "\n",
    "print(is_armstrong(370))\n"
   ]
