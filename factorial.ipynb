    "import math\n",
    "\n",
    "# Numbers to calculate factorial for\n",
    "numbers = [16, 20]\n",
    "\n",
    "for n in numbers:\n",
    "    factorial = math.factorial(n)\n",
    "    print(f\"{n}! = {factorial}\")\n"
